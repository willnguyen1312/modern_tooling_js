{
  "extends": ["airbnb", "prettier"],
  "plugins": ["flowtype"],
  "parser": "babel-eslint",
  "rules": {
    "semi": ["error", "never"],
    "no-console": "off",
    "import/prefer-default-export": "off",
    "react/jsx-filename-extension": "off"
  },
  "env": {
    "jest": true,
    "node": true,
    "browser": true
  },
  "globals": {
    "$Keys": true
  }
}
